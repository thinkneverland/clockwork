{
    "name": "thinkneverland/tapped",
    "description": "Laravel Livewire debugger with real-time browser extension, PHP-based MCP protocol server, and AI IDE/plugin integration hooks",
    "keywords": [
        "debugging",
        "profiling",
        "logging",
        "laravel",
        "livewire",
        "devtools",
        "mcp",
        "ide-integration"
    ],
    "homepage": "https://thinkneverland.com/tapped",
    "license": "MIT",
    "authors": [
        {
            "name": "ThinkNeverland",
            "homepage": "https://thinkneverland.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0",
        "livewire/livewire": "*",
        "ext-json": "*",
        "cboden/ratchet": "^0.4.4",
        "react/event-loop": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "mockery/mockery": "^1.5"
    },
    "suggest": {
        "ext-pdo": "Needed for SQL database metadata storage",
        "ext-redis": "Needed for Redis metadata storage"
    },
    "autoload": {
        "psr-4": {
            "ThinkNeverland\\Tapped\\": "src/",
            "Clockwork\\": "Clockwork/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThinkNeverland\\Tapped\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThinkNeverland\\Tapped\\TappedServiceProvider"
            ],
            "aliases": {
                "Tapped": "ThinkNeverland\\Tapped\\Facades\\Tapped"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
